#!/usr/bin/env sh
. "${HOOKS_DIR}/pingcommon.lib.sh"
if test "${RUN_PLAN}" = "START" || test "${SERVER_PROFILE_UPDATE}" = "true"
then
    echo_yellow "Removing unwanted PF default .jar(s)"
    rm -f "${SERVER_BITS_DIR}/server/default/deploy/pf-pingid-idp-adapter-2.6.jar"
    rm -f "${SERVER_BITS_DIR}/server/default/deploy/PingIDRadiusPCV-2.5.0.jar"
    rm -f "${SERVER_BITS_DIR}/server/default/lib/pf-authn-api-sdk-1.0.0.48.jar"
    if [ -f "${SERVER_BITS_DIR}/server/default/lib/pf-authn-api-sdk-1.0.0.56.jar" ]
    then
        echo_yellow "removing pf-authn-api-sdk-1.0.0.55.jar"
        rm -f "${SERVER_BITS_DIR}/server/default/lib/pf-authn-api-sdk-1.0.0.55.jar"
    else
        echo_yellow "not found pf-authn-api-sdk-1.0.0.56.jar"
    fi
fi
